{
  "name": "peek-server",
  "version": "1.1.3",
  "description": "Backend for Peek Stash Browser. Handles serving the client, API endpoints, communication with media library manager (Stash), and transcoding",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "node dist/index.js",
    "start": "node dist/index.js",
    "dev": "nodemon --legacy-watch --watch './**/*.ts' --watch '../.env' --ext ts --exec 'node --loader ts-node/esm' index.ts",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.17.0",
    "ts-node": "^10.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "stashapp-api": "^0.3.6"
  }
}
