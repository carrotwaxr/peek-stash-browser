{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["index.ts", "api.ts", "prisma/seed.ts"],
  "project": ["**/*.ts"],
  "ignore": [
    "dist/**",
    "node_modules/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "prisma/migrations/**"
  ],
  "ignoreDependencies": [
    "@types/*",
    "typescript",
    "ts-node",
    "nodemon",
    "eslint",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser",
    "vitest",
    "@vitest/ui",
    "@vitest/coverage-v8",
    "prisma",
    "depcheck",
    "knip"
  ],
  "ignoreExportsUsedInFile": true
}
