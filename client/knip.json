{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.jsx",
    "vite.config.js"
  ],
  "project": [
    "src/**/*.{js,jsx,ts,tsx}"
  ],
  "ignore": [
    "dist/**",
    "node_modules/**",
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.spec.{js,jsx,ts,tsx}"
  ],
  "ignoreDependencies": [
    "@vitejs/plugin-react",
    "vite",
    "eslint",
    "vitest",
    "@vitest/ui",
    "@vitest/coverage-v8",
    "happy-dom",
    "depcheck",
    "knip"
  ],
  "vite": {
    "config": ["vite.config.js"]
  }
}
